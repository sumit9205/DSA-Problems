public class Solution {
    public ListNode getIntersectionNode(ListNode headA, ListNode headB) {
   
        
        ListNode ptrA = headA;
        ListNode ptrB = headB;
        
        while(ptrA!=ptrB){
            ptrA = ptrA!=null ? ptrA.next : headB ;
            ptrB = ptrB!=null ? ptrB.next : headA ;
        }
        return ptrA;
}
}
